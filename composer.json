{
    "name": "artox-lab/clarc-bundle",
    "description": "Build Symfony project within Clean Architecture",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Turchin",
            "email": "a.turchin@artox.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "google/protobuf": "^3.3",
        "league/fractal": "^0.20",
        "liip/monitor-bundle": "^2.16",
        "pagerfanta/pagerfanta": "^3.0",
        "symfony-bundles/json-request-bundle": "^4.0",
        "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/messenger": "^5.4 || ^6.0 || ^7.0",
        "symfony/security-bundle": "^5.4 || ^6.0 || ^7.0",
        "symfony/translation": "^5.4 || ^6.0 || ^7.0",
        "symfony/validator": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "haydenpierce/class-finder": "^0.4.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1.31",
        "ramsey/uuid": "^3.0 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ArtoxLab\\Bundle\\ClarcBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    }
}
